{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "CIRCUITS"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SqlServerTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          circuitId as string,",
				"          circuitRef as string,",
				"          name as string,",
				"          location as string,",
				"          country as string,",
				"          lat as string,",
				"          lng as string,",
				"          alt as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CIRCUITS",
				"CIRCUITS derive(circuitId = toInteger(circuitId),",
				"          lat = toFloat(lat),",
				"          lng = toFloat(lng),",
				"          alt = toInteger(alt)) ~> ModifyColumns1",
				"ModifyColumns1 select(mapColumn(",
				"          circuitId,",
				"          circuitRef,",
				"          name,",
				"          location,",
				"          country,",
				"          lat,",
				"          lng,",
				"          alt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 derive(CREATE_DATE = currentUTC('yyyy-MM-dd HH:mm:ss')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CIRCUIT_ID as integer,",
				"          CIRCUIT_REFERENCE as string,",
				"          NAME as string,",
				"          LOCATION as string,",
				"          COUNTRY as string,",
				"          LATITUDE as double,",
				"          ALTITUDE as integer,",
				"          CREATE_DATE as date,",
				"          LONGITUDE as double",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          CIRCUIT_ID = circuitId,",
				"          CIRCUIT_REFERENCE = circuitRef,",
				"          NAME = name,",
				"          LOCATION = location,",
				"          COUNTRY = country,",
				"          LATITUDE = lat,",
				"          ALTITUDE = alt,",
				"          CREATE_DATE,",
				"          LONGITUDE = lng",
				"     )) ~> sink1"
			]
		}
	}
}